#include <iostream>
#include "string.h"
using namespace std;

int main() {
	int n,x=0;
	cin>>n;
	while(n!=0)
	{
		x=x*10+n%10;
		n/=10;
	}
	cout<<x<<"\n"<<3*x;

	return 0;
}
